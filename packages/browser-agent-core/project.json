{
  "name": "browser-agent-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/browser-agent-core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/browser-agent-core",
        "main": "packages/browser-agent-core/src/index.ts",
        "tsConfig": "packages/browser-agent-core/tsconfig.lib.json",
        "assets": ["packages/browser-agent-core/*.md"],
        "buildableProjectDepsInPackageJsonType": "dependencies"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/browser-agent-core/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/browser-agent-core/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
